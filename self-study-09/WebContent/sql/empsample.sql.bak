DROP TABLE EMPLOYEES;

CREATE TABLE EMPLOYEES(
  id VARCHAR2(10) NOT NULL,
  pass VARCHAR2(10) NOT NULL,
  NAME VARCHAR2(24),
  lev VARCHAR2(30),
  enter DATE,
  PRIMARY KEY(id)
);

