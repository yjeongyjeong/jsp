package com.saeyan.dto;

/*
 * NAME            VARCHAR2(20) 
USERID NOT NULL VARCHAR2(10) 
PWD             VARCHAR2(10) 
EMAIL           VARCHAR2(20) 
PHONE           CHAR(13)     
ADMIN           NUMBER(1)
 */
public class Member {
	private String name;
	private String userid;
	private String pwd;
	private String email;
	private String phone;
	private int admin;
}
